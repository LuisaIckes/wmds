    rdfs:label "Very small"@en ,
    "Très faible "@fr ,
    "Muy baja"@es ,
    "Очень малая"@ru ;
    dct:description  "between -100 m and 100 m"@en ,
    "Entre -100 m et 100 m"@fr ,
    "Entre -100 y 100 m"@es ,
    "Меджду -100 м и 100 м"@ru .

<04/small> a skos:Concept ;
