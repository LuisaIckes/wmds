<06/raw> a skos:Concept ;
    skos:notation "raw" ;
    rdfs:label "Raw"@en ,
    "Brut"@fr ,
    "Brutos"@es ,
    "Необработанные"@ru ;
    dct:description  "CEOS: Physical information: Data in their original packets, as received from a satellite"@en ,
