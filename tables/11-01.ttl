    skos:notation "iridiumDoppler" ;
    rdfs:label "IRIDIUM DOPPLER"@en ,
    "IRIDIUM DOPPLER"@fr ,
    "IRIDIUM DOPPLER"@es ,
    "IRIDIUM DOPPLER"@ru ;
    dct:description  "Location determined using the frequency shift of transmissions to the Iridium communications satellite constellation. https://www.iridium.com/"@en ,
